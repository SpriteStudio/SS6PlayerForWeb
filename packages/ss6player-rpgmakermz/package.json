{
  "name": "ss6player-rpgmakermz",
  "private": true,
  "version": "0.8.0",
  "description": "SS6Player for RPG Maker MZ",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi.js",
    "RPG Maker MZ",
    "RMMZ"
  ],
  "main": "dist/ss6player-rpgmakermz.js",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "CRI Middleware Co., Ltd.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "eslint -c ../../.eslintrc.js --ext .js ./src",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "deploy": "npm run build && npx ncp dist/ss6player-rpgmakermz.js SampleProject/js/plugins/ss6player-rpgmakermz.js",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run build",
    "view": "concurrently \"http-server -c-1 -p 8083 ./SampleProject\" \"opener http://localhost:8083/index.html\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ss6player-pixi6": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "typedoc": "^0.25.1",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "rollup-plugin-includepaths": "^0.2.4",
    "@rollup/plugin-json": "^6.0.0",
    "rollup-plugin-license": "^3.1.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "esbuild": "^0.19.3",
    "pixi.js": "^5.3.12",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "opener": "^1.5.2",
    "replace": "^1.2.2",
    "ncp": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.8.1",
    "eslint-config-prettier": "^9.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

{
  "name": "ss6player-viewer",
  "private": true,
  "version": "1.4.0",
  "description": "SS6Player for viewer",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi.js"
  ],
  "main": "dist/ss6player-viewer.umd.js",
  "module": "dist/ss6player-viewer.es5.js",
  "typings": "dist/types/ss6player-viewer.d.ts",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "CRI Middleware Co., Ltd.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "eslint -c ../../.eslintrc.js --ext .ts ./src",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "docs": "typedoc --out ../../docs/ss6player_viewer_api src/ss6player-viewer.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run docs && npm run build",
    "view": "concurrently \"http-server -c-1 -p 8084 ../../\" \"opener http://localhost:8084/packages/ss6player-viewer/Player/index.html\"",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@pixi/display": "^7.2.4",
    "@pixi/graphics": "^7.2.4",
    "@pixi/core": "^7.2.4",
    "@pixi/app": "^7.2.4"
  },
  "dependencies": {
    "ss6player-pixi": "2.0.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "typedoc": "^0.25.0",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "rollup-plugin-includepaths": "^0.2.4",
    "@rollup/plugin-json": "^6.0.0",
    "rollup-plugin-license": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "esbuild": "^0.19.2",
    "pixi.js": "^7.2.4",
    "concurrently": "^8.2.1",
    "http-server": "^14.1.1",
    "opener": "^1.5.2",
    "replace": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@typescript-eslint/eslint-plugin-tslint": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-config-prettier": "^9.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

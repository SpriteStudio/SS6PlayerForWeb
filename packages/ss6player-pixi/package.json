{
  "name": "ss6player-pixi",
  "version": "1.7.0",
  "description": "SS6Player for pixi",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi"
  ],
  "main": "dist/ss6player-pixi.umd.js",
  "module": "dist/ss6player-pixi.es5.js",
  "typings": "dist/types/ss6player-pixi.d.ts",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Web Technology Corp.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "docs": "typedoc --out ../../docs/ss6player_pixi_api src/ss6player-pixi.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run docs && npm run build",
    "view": "concurrently \"http-server -c-1 ../../\" \"opener http://localhost:8080/packages/ss6player-pixi/Player/index.html\"",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@pixi/loaders": "^6.1.3",
    "@pixi/display": "^6.1.3",
    "@pixi/mesh-extras": "^6.1.3",
    "@pixi/ticker": "^6.1.3",
    "@pixi/filter-color-matrix": "^6.1.3",
    "@pixi/core": "^6.1.3",
    "@pixi/constants": "^6.1.3"
  },
  "dependencies": {
    "ssfblib": "1.1.0"
  },
  "devDependencies": {
    "typescript": "^4.4.4",
    "typedoc": "^0.22.9",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "rollup-plugin-includepaths": "^0.2.4",
    "@rollup/plugin-json": "^4.1.0",
    "rollup-plugin-license": "^2.6.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "rollup-plugin-esbuild": "^4.6.0",
    "esbuild": "^0.13.14",
    "@types/offscreencanvas": "^2019.6.4",
    "pixi.js": "^6.1.3",
    "concurrently": "^6.3.0",
    "http-server": "^14.0.0",
    "opener": "^1.5.2",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

{
  "name": "ss6player-pixi",
  "private": true,
  "version": "2.0.0",
  "description": "SS6Player for pixi",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi"
  ],
  "main": "dist/ss6player-pixi.umd.js",
  "module": "dist/ss6player-pixi.es5.js",
  "typings": "dist/types/ss6player-pixi.d.ts",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "CRI Middleware Co., Ltd.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "eslint -c ../../.eslintrc.js --ext .ts ./src",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --configPlugin typescript --bundleConfigAsCjs",
    "docs": "typedoc --out ../../docs/ss6player_pixi_api src/ss6player-pixi.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run docs && npm run build",
    "view": "concurrently \"http-server -c-1 ../../\" \"opener http://localhost:8080/packages/ss6player-pixi/Player/index.html\"",
    "sample-ui": "concurrently \"http-server -c-1 ../../\" \"opener http://localhost:8080/packages/ss6player-pixi/examples/ui/index.html\"",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@pixi/assets": "^7.2.4",
    "@pixi/display": "^7.2.4",
    "@pixi/mesh-extras": "^7.2.4",
    "@pixi/ticker": "^7.2.4",
    "@pixi/filter-color-matrix": "^7.2.4",
    "@pixi/core": "^7.2.4",
    "@pixi/constants": "^7.2.4"
  },
  "dependencies": {
    "ss6player-lib": "1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "typedoc": "^0.24.8",
    "flatbuffers": "^23.5.26",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "rollup-plugin-includepaths": "^0.2.4",
    "@rollup/plugin-json": "^6.0.0",
    "rollup-plugin-license": "^3.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "esbuild": "^0.19.2",
    "pixi.js": "^7.2.4",
    "concurrently": "^8.2.0",
    "http-server": "^14.1.1",
    "opener": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-config-prettier": "^9.0.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}

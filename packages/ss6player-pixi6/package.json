{
  "name": "ss6player-pixi6",
  "private": true,
  "version": "1.0.3",
  "description": "SS6Player for pixi",
  "keywords": [
    "SpriteStudio",
    "SS6Player",
    "2D",
    "Animation",
    "Sprite",
    "Skeletal",
    "pixi"
  ],
  "main": "dist/ss6player-pixi6.umd.js",
  "module": "dist/ss6player-pixi6.es6.js",
  "typings": "dist/types/ss6player-pixi6.d.ts",
  "bugs": {
    "url": "https://github.com/SpriteStudio/SS6PlayerForWeb/issues"
  },
  "homepage": "https://github.com/SpriteStudio/SS6PlayerForWeb#readme",
  "files": [
    "dist"
  ],
  "author": {
    "name": "CRI Middleware Co., Ltd.",
    "url": "https://www.webtech.co.jp/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SpriteStudio/SS6PlayerForWeb.git"
  },
  "license": "SEE LICENSE",
  "scripts": {
    "start": "npm run watch",
    "lint": "eslint -c ../../eslint.config.js ./src",
    "prebuild": "npm run clean",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --configPlugin typescript --bundleConfigAsCjs --strictDeprecations",
    "docs": "typedoc --out ../../docs/ss6player_pixi6_api src/ss6player-pixi6.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "dist": "npm run docs && npm run build",
    "view": "concurrently \"http-server -c-1 -p 8086 ../../\" \"opener http://localhost:8086/packages/ss6player-pixi6/Player/index.html\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "ss6player-lib": "1.0.3"
  },
  "devDependencies": {
    "typescript": "^5.8.2",
    "typedoc": "^0.28.1",
    "lodash.camelcase": "^4.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.37.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@rollup/plugin-commonjs": "^28.0.3",
    "rollup-plugin-includepaths": "^0.2.4",
    "@rollup/plugin-json": "^6.1.0",
    "rollup-plugin-license": "^3.6.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "esbuild": "^0.25.1",
    "pixi.js": "^6.5.10",
    "@pixi/loaders": "^6.5.10",
    "@pixi/display": "^6.5.10",
    "@pixi/mesh-extras": "^6.5.10",
    "@pixi/ticker": "^6.5.10",
    "@pixi/filter-color-matrix": "^6.5.10",
    "@pixi/core": "^6.5.10",
    "@pixi/constants": "^6.5.10",
    "concurrently": "^9.1.2",
    "http-server": "^14.1.1",
    "opener": "^1.5.2",
    "eslint": "^9.23.0",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  }
}

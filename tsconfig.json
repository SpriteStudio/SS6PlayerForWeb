{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es6",
    "module":"es2022",
    "lib": ["es2015", "es2016", "es2017", "es2020", "es2022", "dom"],
    "strict": false,
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  }
}
